func add: (r0:point,r1:point)=>point{
  var ret: point;
  ret.x = r0.x + r1.x;
  ret.y = r0.y + r1.y;
  ret.z = r0.z + r1.z;
  return ret;
}

func center:(t:triangle)=>point{
  var ret: point;
  ret.x = (t.p0.x + t.p1.x + t.p2.x) // 3;
  ret.y = (t.p0.y + t.p1.y + t.p2.y) // 3;
  ret.z = (t.p0.z + t.p1.z + t.p2.z) // 3;
  return ret;
}

